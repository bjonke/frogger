<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="frogger" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option show_notes="0">
			<notes>
				<![CDATA[Frogger old arcade game]]>
			</notes>
		</Option>
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/frogger" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-lSDL2 -lSDL2_ttf -lSDL2_image" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/map" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-lSDL2 -lSDL2_ttf -lSDL2_image" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add option="`sdl-config --cflags`" />
		</Compiler>
		<Linker>
			<Add option="`sdl-config --libs`" />
		</Linker>
		<Unit filename="car.h" />
		<Unit filename="enemy.h" />
		<Unit filename="life.h" />
		<Unit filename="main.cpp" />
		<Unit filename="treetrunk.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
